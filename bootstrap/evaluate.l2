				[make-object-symbol (" -g-)(character(char g)) $obj-buf]
				[make-object-symbol (" -h-)(character(char h)) $obj-buf]
				[make-object-symbol (" -i-)(character(char i)) $obj-buf]
				[make-object-symbol (" -j-)(character(char j)) $obj-buf]
				[make-object-symbol (" -k-)(character(char k)) $obj-buf]
				[make-object-symbol (" -l-)(character(char l)) $obj-buf]
				[make-object-symbol (" -m-)(character(char m)) $obj-buf]
				[make-object-symbol (" -n-)(character(char n)) $obj-buf]
				[make-object-symbol (" -o-)(character(char o)) $obj-buf]
				[make-object-symbol (" -p-)(character(char p)) $obj-buf]
				[make-object-symbol (" -q-)(character(char q)) $obj-buf]
				[make-object-symbol (" -r-)(character(char r)) $obj-buf]
				[make-object-symbol (" -s-)(character(char s)) $obj-buf]
				[make-object-symbol (" -t-)(character(char t)) $obj-buf]
				[make-object-symbol (" -u-)(character(char u)) $obj-buf]
				[make-object-symbol (" -v-)(character(char v)) $obj-buf]
				[make-object-symbol (" -w-)(character(char w)) $obj-buf]
				[make-object-symbol (" -x-)(character(char x)) $obj-buf]
				[make-object-symbol (" -y-)(character(char y)) $obj-buf]
				[make-object-symbol (" -z-)(character(char z)) $obj-buf]
				[make-object-symbol (" -|-)(character(char |)) $obj-buf]
				[make-object-symbol (" -~-)(character(char ~)) $obj-buf]
				[make-object-symbol (" @fst) @car $obj-buf]
				[make-object-symbol (" @rst) @cdr $obj-buf]
				[make-object-symbol (" lst) cons $obj-buf]
				[make-object-symbol (" symbol?) i/f:symbol? $obj-buf]
				[make-object-symbol (" emt?) nil? $obj-buf]
				[make-object-symbol (" emt) nil $obj-buf]
				[make-object-symbol (" char=) i/f:char= $obj-buf] $obj-buf) $obj-buf $expr-buf handler err])
			
		(handler
			(switch = [@0 err]
				(multiple-definition-error (begin
					[write-str (stdout) (" The definition of(space)) cleanup err]
					[write-str (stdout) [@1 err] cleanup err]
					[write-str (stdout) (" (space)erroneously occured multiple times.(lf)) cleanup err]))
					
				(param-count-mismatch-error (begin
					[write-str (stdout) (" The number of arguments in(space)) cleanup err]
					[print-expression [@1 err]]
					[write-str (stdout) (" (space)does not match the number of parameters in(space)) cleanup err]
					[print-expression [@2 err]]
					[write-str (stdout) (" .(lf)) cleanup err]))
				
				(undefined-reference-error (begin
					[write-str (stdout) (" Undefined reference:(space)) cleanup err]
					[write-str (stdout) [@1 err] cleanup err]
					[write-str (stdout) (" .(lf)) cleanup err]))
				
				(special-form-error (if [@2 err]
					(begin
						[write-str (stdout) (" The subexpression(space)) cleanup err]
						[print-fragment [@2 err]]
						[write-str (stdout) (" (space)does not satisfy the requirements of the expression(space)) cleanup err]
						[print-fragment [@1 err]]
						[write-str (stdout) (" .(lf)) cleanup err])
					(begin
						[write-str (stdout) (" The expression(space)) cleanup err]
						[print-fragment [@1 err]]
						[write-str (stdout) (" (space)has an incorrect number of subexpressions.(lf)) cleanup err])))
				
				(unexpected-char-error (begin
					[write-str (stdout) (" Unexpectedly read(space)) cleanup err]
					[write-char (stdout) [@1 err] cleanup err]
					[write-str (stdout) (" (space)at(space)) cleanup err]
					[write-uint (stdout) [@2 err] cleanup err]
					[write-str (stdout) (" .(lf)) cleanup err]))
				
				(file-access-error (if [@1 err]
					(begin
						[write-str (stdout) (" Inaccessable file:(space)) cleanup err]
						[write-str (stdout) [@1 err] cleanup err]
						[write-str (stdout) (" .(lf)) cleanup err])
					[write-str (stdout) (" Inaccessable file.) cleanup err]))
				
				(unsupported-ext-error (begin
					[write-str (stdout) (" File has unsupported extension:(space)) cleanup err]
					[write-str (stdout) [@1 err] cleanup err]
					[write-str (stdout) (" .(lf)) cleanup err]))
					
				(begin)))
			
			(cleanup (begin))
			
			(cleanup-expr-buf [destroy-buffer $expr-buf])
			
			(cleanup-obj-buf [destroy-buffer $obj-buf])
			
			(_ [exit (if [= [@0 err] no-error] #0 #1)])))
