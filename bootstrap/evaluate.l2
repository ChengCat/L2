				[make-object-symbol (" -Y-)(character(char Y)) $obj-buf]
				[make-object-symbol (" -Z-)(character(char Z)) $obj-buf]
				[make-object-symbol (" -\-)(character(char \)) $obj-buf]
				[make-object-symbol (" -^-)(character(char ^)) $obj-buf]
				[make-object-symbol (" -_-)(character(char _)) $obj-buf]
				[make-object-symbol (" -`-)(character(char `)) $obj-buf]
				[make-object-symbol (" -a-)(character(char a)) $obj-buf]
				[make-object-symbol (" -b-)(character(char b)) $obj-buf]
