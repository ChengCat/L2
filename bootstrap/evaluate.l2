				[make-object-symbol (" -J-)(character(char J)) $obj-buf]
				[make-object-symbol (" -K-)(character(char K)) $obj-buf]
				[make-object-symbol (" -L-)(character(char L)) $obj-buf]
				[make-object-symbol (" -M-)(character(char M)) $obj-buf]
				[make-object-symbol (" -N-)(character(char N)) $obj-buf]
				[make-object-symbol (" -O-)(character(char O)) $obj-buf]
