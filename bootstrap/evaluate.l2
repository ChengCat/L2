				[make-object-symbol (" -+-)(character(char +)) $obj-buf]
				[make-object-symbol (" -,-)(character(char ,)) $obj-buf]
				[make-object-symbol (" ---)(character(char -)) $obj-buf]
				[make-object-symbol (" -.-)(character(char .)) $obj-buf]
				[make-object-symbol (" -/-)(character(char /)) $obj-buf]
				[make-object-symbol (" -0-)(character(char 0)) $obj-buf]
				[make-object-symbol (" -1-)(character(char 1)) $obj-buf]
