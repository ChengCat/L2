(function switch (l)
	(`(let (tempeq0 (,[fst [' l]])) (tempval0 (,[frst [' l]]))
		(,(with return
			{(continuation aux (remaining else-clause)
				(if [nil? [' remaining]]
					{return [' else-clause]}
					{aux [rst [' remaining]]
						(`(if (,[llllst (` invoke) (` [' tempeq0]) (` [' tempval0]) [ffst [' remaining]] [nil]])
							(,[frfst [' remaining]]) (,[' else-clause])))}))
				[rst [reverse [rrst [' l]]]] [fst [reverse [' l]]]})))))
